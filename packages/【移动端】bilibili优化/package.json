{
	"name": "m-bilibili-script",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vue-tsc --noEmit && vite",
		"build": "vue-tsc --noEmit && vite build",
		"build:min": "vue-tsc --noEmit && vite build --minify",
		"build:all": "vue-tsc --noEmit && vite build && vite build --minify --no-empty-outDir",
		"build:watch": "vue-tsc --noEmit && vite build --watch",
		"build:no-cache": "pnpm store prune && vue-tsc --noEmit && vite build",
		"preview": "vue-tsc --noEmit && vite preview --host=0.0.0.0",
		"clear": "del .\\SCRIPT_VERSION.json && pnpm store prune"
	},
	"dependencies": {
		"@element-plus/icons-vue": "^2.3.1",
		"@mgdn/bvid": "^0.2.1",
		"@whitesev/domutils": "^1.1.1",
		"@whitesev/utils": "^1.4.4",
		"md5": "^2.3.0",
		"qmsg": "^1.1.2",
		"vue": "^3.4.29",
		"vue-router": "^4.3.3"
	},
	"devDependencies": {
		"@types/md5": "^2.3.5",
		"@types/node": "^20.14.2",
		"@vitejs/plugin-vue": "^5.0.5",
		"element-plus": "^2.7.5",
		"typescript": "^5.4.5",
		"unplugin-auto-import": "^0.17.6",
		"unplugin-element-plus": "^0.8.0",
		"unplugin-icons": "^0.19.0",
		"unplugin-vue-components": "^0.27.0",
		"vite": "^5.3.1",
		"vite-plugin-monkey": "4.0.3",
		"vue-tsc": "^2.0.21"
	}
