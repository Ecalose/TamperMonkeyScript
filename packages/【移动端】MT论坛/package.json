    "element-plus": "^2.7.2",
    "typescript": "^5.4.5",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-element-plus": "^0.8.0",
    "unplugin-icons": "^0.19.0",
    "unplugin-vue-components": "^0.27.0",
